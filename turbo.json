{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "dev": {
            "dependsOn": ["^dev"],
            "outputs": ["dist/**"]
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "test": {
            "dependsOn": ["build"],
            "outputs": []
        }
    }
}
